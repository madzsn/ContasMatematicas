#include <iostream>
int main ()  {

int a = 523;
int b = 90;

int soma = a + b;       // soma = 613
int subtracao = a - b;  // subtracao = 433
int multiplicacao = a * b; // multiplicacao = 47070
int divisao = a / b; // divisao = 5,81

std::cout << "soma" << soma << std::endl;
std::cout << "subtracao" << subtracao << std::endl;
std::cout << "multiplicacao" << multiplicacao << std::endl;
std::cout << "divisao" << divisao << std::endl;



return 0;
}




